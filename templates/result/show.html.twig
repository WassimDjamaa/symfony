{% extends 'base.html.twig' %}

{% block title %}Result{% endblock %}

{% block body %}
    <h1>Result</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ result.id }}</td>
            </tr>
            <tr>
                <th>Result_state</th>
                <td>{{ result.resultState ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Adress_ip</th>
                <td>{{ result.adressIp }}</td>
            </tr>
        </tbody>
